{
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",  
      "handler": "handler",     
      "maxDuration": 30         
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"  
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"   
    }
  ]
}